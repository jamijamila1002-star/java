import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        int d=sc.nextInt();
        System.out.println("Items Details");
         System.out.printf("A4sheet:%d\n",a);
        System.out.printf("Pen:%d\n",b);
        System.out.printf("Pencil:%d\n",c);
        System.out.printf("Eraser:%d\n",d);
        }
}
